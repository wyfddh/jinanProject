}
.layui-btn{
    line-height: 28px;
    height:28px;
    background:rgba(146,164,233,1);
    border-radius:2px;
    font-size:14px;
    font-family:MicrosoftYaHei;
    color:rgba(255,255,255,1);
}
.layui-layedit-tool{
    background-color: #EEEFF4;
}


.uploadBtn{
    display: block;
    width:157px;
    height:111px;
    background:rgba(224,228,238,1);
    border-radius:8px;
    line-height:20px;
    font-family:MicrosoftYaHei;
    padding-top: 5px;
    cursor: pointer;
    float: left;
    margin-left: 20px;
}
.uploadIcon{
    display: block;
    text-align: center;
    margin:10px auto;
    width:30px;
    height:30px;
    border-radius:50%;
    background:url(../../images/add_def.png) no-repeat;
    border:none;
}
.uploadTxt{
    display: block;
    text-align: center;
    font-size:14px;
    line-height:20px;
    margin-bottom:5px;
    color:#575962;
}
.uploadTip{
    display: block;
    text-align: center;
    margin:0 auto;
    width:84px;
    height:32px;
    font-size:12px;
    color:#ACAEB0;
    line-height:16px;
}

.uploadBtn:hover{
    background:#EEF5FD;
}
.uploadBtn:active{
    background:#E0E4EE;
}
.uploadBtn:active .uploadIcon{
    background-image:url(../../images/add.png);
}

.layui-upload-img{
    width: 157px;
    height: 116px;
    border-radius: 8px;
}

.msg{
    padding: 24px;
    border-radius: 8px;
    font-family:MicrosoftYaHei;
}
.msg-icon{
    margin:0 auto;
    text-align:center;
    width:72px;
    height:72px;
}
.successMsg .msg-icon{
    background:url(../../images/msg_success.png) no-repeat center center;
}
.errorMsg .msg-icon{
    background:url(../../images/msg_error.png) no-repeat center center;
}
.alertMsg .msg-icon{
    background:url(../../images/msg_alert.png) no-repeat center center;
}
.loadingMsg .msg-icon{
    background:url(../../images/msg_loading.png) no-repeat center center;
    transform-origin: center center;
    animation: rotate 1s linear infinite;
}
.msg-title{
    margin-top:12px;
    text-align:center;
    font-size:16px;
    line-height:30px;
    color:#2B2C2F;
}
.msg-txt{
    text-align:center;
    font-size:14px;
    line-height:24px;
    color:#ACAEB0;
}
.img{
    display: block;
    width: 157px;
    height: 116px;
    background: rgba(224,228,238,1);
    border-radius: 8px;
    line-height: 20px;
    cursor: pointer;
    float: left;
    margin-left:20px;
    overflow: hidden;
    margin-bottom: 20px;
}
.img1{
    width: 157px;
    height: 91px;
}
.img1 img{
    width: 157px;
    height: 91px;
}
.img2{
    width: 157px;
    height: 25px;
    line-height:25px;
    text-align:center;
    font-size: 13px;
}
.img2 span{
    display: inline-block;
}
.img3{
    margin-right: 10px;
}
@keyframes rotate {
    0%{
        transform: rotate(0deg);
    }
    25%{
        transform: rotate(90deg);
    }
    50%{
        transform: rotate(180deg);
    }
    75%{
        transform: rotate(270deg);
    }
    100%{
        transform: rotate(360deg);
    }
}
