body {
    padding: 27px 36px 0;
}
.icon {
    height: 301px;
    background-color: #f9fbfd;
    text-align: center;
    border: 1px solid #e5e5e5;
    box-sizing: border-box;
}
body>h3{
    margin-bottom: 30px;
    font-size: 20px;
    margin-top: 50px;
}
body>h3:first-child{
    margin-top: 0;
}

.icon img{
    width: 100px;
    height: 100px;
    border-radius: 50%;
    margin-top: 60px;
    margin-bottom: 35px;
}
.icon p {
    margin-bottom: 25px;
    font-size: 20px;
}

.icon span {
    padding: 10px 16px;
    background-color: #ffaa4c;
    border-radius: 20px;
    font-size: 14px;
}

.info {
    box-sizing: border-box;
}

.info.top{
    border-top: 1px solid #e5e5e5;
}

.info div{
    height: 50px;
    line-height: 50px;
    border-bottom: 1px solid #e5e5e5;
    border-right: 1px solid #e5e5e5;
    box-sizing: border-box;
    font-size: 13px;
}

.info div:nth-child(2n+1) {
    text-align: right;
    padding-right: 25px;
    background-color: #f9fbfd;
}

.info div:nth-child(2n) {
    text-align: left;
    padding-left: 25px;
}
.count{
    width: 100%;
}
.count th{
    width: 20%;
    height: 56px;
    background-color: #f5f5f5;
    text-align: center;
    box-sizing: border-box;
}
.count td {
    width: 20%;
    height: 56px;
    text-align: center;
    box-sizing: border-box;
}

.list {
    text-align: center;
}

.list th{
    text-align: center;
}