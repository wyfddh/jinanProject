<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>藏品管理</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="format-detection" content="telephone=no">
    <link rel="stylesheet" href="../../layui/css/layui.css" media="all" />
    <link rel="stylesheet" href="../../css/public.css" media="all" />
    <link rel="stylesheet" href="../../css/commonList.css" media="all" />
    <link rel="stylesheet" href="../../css/esaleCollectionInfo/esaleCollectionInfoList.css" media="all" />
    <link rel="stylesheet" href="../../lib/lightbox2/2.8.1/css/lightbox.css" type="text/css">
    <style>

    </style>
</head>
<body class="childrenBody">
<form class="layui-form g-list">
    <div class="top">
        <div class="top_line">
            <!--<img src="../../images/chaxun.png" alt="" style="margin-left: 20px;margin-right: 8px">-->
            &nbsp;&nbsp;&nbsp;&nbsp;<i class="layui-icon">&#xe615;</i> <span>筛选查询</span>
            <div class="shouqi"><img class="shou1" src="../../images/arrow.png" alt=""><span>收起筛选</span></div>
            <div class="layui-btn search_btn cha g-btn-search">查询</div>
            <div class="re" >
                <button type="reset" id="resetBtn"  class="layui-btn rebtn g-btn-search">重置</button>
            </div>
        </div>
        <div class="top_body">
            <div class="tb1">
                <label class="tb2">输入搜索：</label>
                <div class="layui-input-inline a1">
                    <input type="text" name="key" id="key" class="layui-input searchVal" autocomplete="off" placeholder="藏品名称/博物馆名称" />
                </div>
            </div>
            <div class="tb1">
                <label class="tb2">藏品类别：</label>
                <div class="layui-input-inline a1">
                    <select name="collectionType" id="collectionType" lay-search>
                        <option value="">请选择类别</option>
                    </select>
                </div>
            </div>
            <div class="tb1">
                <label class="tb2">年代：</label>
                <div class="layui-input-inline a1">
                    <select name="collectionYear" id="collectionYear" lay-verify="required">
                        <option value="">请选择</option>
                    </select>
                </div>
            </div>
        </div>
    </div>
    <div class="tab">
        <div class="tab_line g-tab_line">
            <img class="tab-top-icon" src="../../statics/images/videoList/Shape2.png" alt="">
            <span class="g-table-title">数据列表</span>
            <!--&nbsp;&nbsp;&nbsp;&nbsp;<i class="layui-icon">&#xe62d;</i><span>数据列表</span>-->
            <!--<div class="add recommendRule" style="cursor:pointer;">推荐规则</div>-->
            <div class="paix">
                <div class="layui-input-inline a2 g-btn-default">
                    <select name="orderBy" id="orderBy" lay-verify="required" lay-filter="orderBy">
                        <option value="">排序方式</option>
                        <option value="2">创建时间</option>
                        <option value="1">更新时间</option>
                    </select>
                </div>
            </div>
        </div>
    </div>
    <div class="cont">


        <table id="collectionManager" lay-filter="collectionManager"></table>
    </div>
    <!-- 图片 -->
    <script type="text/html" id="picUrl">
        <a href="{{d.picUrl}}" data-lightbox="gallery">
            <img alt="没有图片" class="bbc" src="{{d.picUrl}}" >
        </a>
    </script>
    <!--推荐-->
    <script type="text/html" id="recommendBar">
        <div>
            <div>
                热门推荐：<input type="checkbox" value="{{d.id}}" name="hotRecommend" {{d.hotRecommendCheck}}  lay-skin="switch" lay-filter="recommend">
            </div>
        </div>
    </script>

    <!--操作-->
    <script type="text/html" id="collectionManagerBar">
        <div>
            {{#  if(d.dataState == 1 ){ }}
            <a class="layui-btn layui-btn-xs" lay-event="up">上线</a>
            {{#  }else { }}
            <a class="layui-btn layui-btn-xs" lay-event="down">下线</a>
            {{#  }  }}
            <a class="layui-btn layui-btn-xs" lay-event="show">查看</a>
            <a class="layui-btn layui-btn-xs" lay-event="del">删除</a>
        </div>
    </script>
</form>
<!--<script type="text/javascript" src="../../layui/layui.js"></script>-->
<script type="text/javascript" src="../../js/jquery.min.js"></script>
<script type="text/javascript" src="../../statics/plugins/layui/layui.js"></script>
<script type="text/javascript" src="../../js/common.js"></script>
<script type="text/javascript" src="../../lib/lightbox2/2.8.1/js/lightbox-plus-jquery.min.js"></script>
<script type="text/javascript" src="../../js/esaleCollectionInfo/esaleCollectionInfoList.js"></script>
</body>
</html>